       CASE WHEN
       (SUM(S.ItemQty) -
                 (COALESCE((SELECT SUM(COALESCE(OLPS.ItemQty,0))
                     FROM v_Other_LocationPicking_Sim AS OLPS
                     WHERE S.IdItem = OLPS.IdItem
