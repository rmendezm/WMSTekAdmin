       HAVING (SUM(SP.ItemQty) - COALESCE(SUM(VOLP.ItemQty),0) - COALESCE(SUM(VOLPS.ItemQty),0)) &gt;= @itemQty2
       --ORDER BY Ruta ASC
       ORDER BY CASE WHEN  @outboundTypeCode = @outboundTypeCodeDoc  THEN L.IdHng END DESC,
       ItemQty,Ruta
