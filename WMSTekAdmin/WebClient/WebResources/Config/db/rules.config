   <query code="STKPUTItemNoZoneGreaterQtyUomCajaCalzada">
     <sql>
       SELECT TOP 1
       ABS(L.PickingFlow - @pickingFlow) AS Ruta,
       S.IdWhs,
       S.IdOwn,
       (IU.ConversionFactor * ROUND((@itemQty/IU.ConversionFactor),0,1)) AS ItemQty,
       S.IdItem,
       S.LotNumber,
       S.fabricationDate,
       S.ExpirationDate,
       S.FifoDate,
