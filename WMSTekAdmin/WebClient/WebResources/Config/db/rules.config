                     OR(S.FabricationDate IS NULL)
                     OR(OLPS.FabricationDate IS NULL))
                     AND L.IdLocCode = OLPS.IdLocSourceProposal),0)
       ) &gt;= CASE WHEN (@itemQty/IU.ConversionFactor) &lt;= 0
