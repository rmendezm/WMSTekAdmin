       LT.LocTypeName, LT.Status,S.LotNumber,S.FabricationDate,S.ExpirationDate
       HAVING (SUM(SP.ItemQty) - COALESCE(SUM(VOLP.ItemQty),0) - COALESCE(SUM(VOLPS.ItemQty),0)) &gt;= @itemQty2
       ORDER BY s.expirationDate ASC, Ruta ASC
