   <query code="STKPICKItemNoZoneGreaterQtyPlanUnid">
     <sql>
		 SELECT TOP 1
		 ABS(L.PickingFlow - @pickingFlow) AS Ruta,
		 L.IdWhs,
		 I.IdOwn,
		 @itemQty1 AS ItemQty,
		 I.IdItem,
		 S.IdLpnCode,
		 S.LpnTypeCode,
		 S.IdLpnCodeContainer,
