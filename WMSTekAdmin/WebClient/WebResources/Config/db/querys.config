      AND C.IdOwn = OO.IdOwn
      INNER JOIN CustomerB2B CB2B
      ON CB2B.IdCustomer = C.IdCustomer
      LEFT JOIN ItemCustomer IC
      ON IC.IdItem = I.IdItem
      AND IC.IdCustomer = C.IdCustomer
      INNER JOIN ItemUom IU
