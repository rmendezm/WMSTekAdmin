          W.IdWhs,
          I.IdItem,
          I.ItemCode,
          I.ShortItemName,
          I.Description,
          B.IdBranch,
          B.BranchCode,
          B.BranchName
          FROM UsefulLife US
          INNER JOIN Customer C ON US.IdCustomer = C.IdCustomer
          INNER JOIN Owner O ON O.IdOwn = US.IdOwn
          INNER JOIN Warehouse W ON W.IdWhs = US.IdWhs
          INNER JOIN Item I ON I.IdItem = US.IdItem
