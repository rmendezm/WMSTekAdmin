          (
          CASE
          WHEN T.TaskTypeCode IN ('PUT', 'RCPA') THEN
          (SELECT IdInboundType AS IdOutboundType FROM InboundOrder IO WHERE IO.IdInboundOrder = TD.IdDocumentBound)
