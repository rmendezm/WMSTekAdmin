          INNER JOIN Warehouse W ON W.IdWhs = O.IdWhs
          WHERE DD.IdWmsProcessCode IN ('PKBAT','PKKIT','PKPSS','PKUNK','PKVAS','PKWAV','PICK','PKLPN')
