          NULL AS SpecialField2Outbound, --> PrefixCodBarraBigticket
          NULL AS SpecialField3Outbound, --> CustomField1
          NULL AS SpecialField4Outbound --> CustomField2
          FROM TaskDetail TD INNER JOIN
