          GROUP BY D.IdOutboundOrder, DD.IdDispatch, DD.IdLpnCOde, L.WeightTotal) Bultos
          ON OO.IdOutboundOrder = Bultos.IdOutboundOrder
          INNER JOIN Owner O
          ON OO.IdOwn = O.IdOwn
          INNER JOIN City C
          ON OO.IdCityDelivery = C.IdCity
          WHERE Bultos.IdLpnCOde = @idLpnCOde
