          LEFT JOIN Customer Cus ON Out.CustomerCode = Cus.CustomerCode AND Out.IdOwn = Cus.IdOwn
          LEFT JOIN Branch Bra ON Out.IdBranch = Bra.IdBranch AND Out.IdOwn = Bra.IdOwn AND Cus.IdCustomer = Bra.IdCustomer

