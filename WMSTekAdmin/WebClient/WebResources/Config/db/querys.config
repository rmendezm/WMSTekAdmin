  <query code="CfgParameterOwner_ByOwner">
    <sql>
      SELECT     
             Wo.IdOwn
            ,Wo.OwnName
            ,Wo.IdParameter
            ,Wo.IdModule
            ,m.Name AS ModuleName
            ,Wo.ParameterCode
            ,Wo.Description
            ,Wo.ParameterValue
            ,Wo.Type
            ,Wo.MinValue
            ,Wo.MaxValue
            ,Wo.DefaultValue
            ,Wo.AllowEdit
            ,Wo.AllowDisplay
            ,Wo.Scope
            ,Wo.DateCreated
            ,Wo.UserCreated
            ,Wo.DateModified
            ,Wo.UserModified
       FROM v_Cfg_WmsOwnerParamByOwn Wo
       JOIN CfgModule m 
         ON Wo.IdModule = m.IdModule
      WHERE IdOwn = @IdOwn
        AND Wo.ParameterCode = @ParameterCode 
    </sql>
    <parameters>
      <parameter type="Int">IdOwn</parameter>
      <parameter type="String">ParameterCode</parameter>
    </parameters>
  </query>

