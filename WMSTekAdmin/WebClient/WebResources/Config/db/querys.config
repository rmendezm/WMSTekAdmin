          , W.WhsCode
          , W.ShortWhsName
          , L.LocCode
          , L.IdHng
          , H.HngCode
          , H.HngName
          , L.RowLoc
          , L.ColumnLoc
          , L.LevelLoc
          , L.Aisle
          , L.Status
          , T.IdLocType
