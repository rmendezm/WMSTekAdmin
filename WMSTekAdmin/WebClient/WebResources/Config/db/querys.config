      LEFT JOIN OutboundDetail OutD ON Out.IdOutboundOrder = OutD.IdOutboundOrder
      LEFT JOIN Item itm ON itm.IdItem = OutD.IdItem
      JOIN v_Oub_LatestTrack tra ON Out.IdOutboundOrder = tra.IdOutboundOrder
