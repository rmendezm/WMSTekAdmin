  <query code="Vas_FindAll">
    <sql>
      SELECT V.IdVas,
      V.IdOwn,
      O.OwnCode,
      O.OwnName,
      O.TradeName,
      V.VasName,
      V.VasDescription,
      V.Status,
      C.CustomerCode,
      C.CustomerName
      FROM Vas V
      INNER JOIN Owner O
      ON O.IdOwn = V.IdOwn
