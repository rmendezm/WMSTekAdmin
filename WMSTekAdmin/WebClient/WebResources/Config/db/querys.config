    <query code="UsefulLife_FindAll">
        <sql>
          SELECT
          US.IdUsefulLife,
          US.DayQty,
          C.CustomerName,
          C.CustomerCode,
          C.IdCustomer,
          O.OwnCode,
          O.OwnName,
          O.IdOwn,
          W.WhsName,
