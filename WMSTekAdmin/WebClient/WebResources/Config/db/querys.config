          ,inDet.UserModified
          ,NULL Lote
          ,NULL Vencimiento
          ,inDet.ItemQty Copias
          FROM InboundDetail inDet
          INNER JOIN Item itm ON itm.IdItem = inDet.IdItem
          LEFT JOIN CategoryItem Ctg ON Ctg.IdCtgItem = inDet.IdCtgItem
          WHERE IdInboundOrder = @idInboundOrder;

