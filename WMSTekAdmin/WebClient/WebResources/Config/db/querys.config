          Item I ON TD.IdItem = I.IdItem LEFT JOIN
          Location L ON L.IdLocCode = TD.IdLocSourceProposal
          AND L.IdWhs = TD.IdWhs LEFT JOIN
          ItemUom IU ON I.IdItem = IU.IdItem
          AND IU.UomName = 'UNIDAD' INNER JOIN
