      <query code="User_findOwners">
        <sql>
          SELECT U.IdOwn
          ,U.IdUserWms
          ,O.OwnCode
          ,O.OwnName
          ,O.TradeName
          ,U.DateCreated
          ,U.UserCreated
          ,U.DefaultOwner
          FROM UserOwner U
          INNER JOIN Owner O ON O.IdOwn = U.IdOwn
          WHERE IdUserWms = @IdUserWms
          ORDER BY O.OwnName
        </sql>
        <parameters>
          <parameter type="Int">IdUserWms</parameter>
        </parameters>
      </query>

