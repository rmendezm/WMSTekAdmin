          AND OO.IdWhs = RD.IdWhs 
          AND OO.IdOwn = RD.IdOwn 
         JOIN Owner O 
           ON OO.IdOwn = O.IdOwn 
         JOIN Warehouse W 
		   ON OO.IdWhs = W.IdWhs
		 JOIN DispatchDetail DD
           ON DD.IdDispatch = D.IdDispatch
		 JOIN Item I
           ON I.IdItem = DD.IdItem
		  AND I.IdOwn = OO.IdOwn
